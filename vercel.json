{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Type",
					"value": "text/html"
				}
			]
		},
		{
			"source": "/assets/js/main.js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		}
	]
}
